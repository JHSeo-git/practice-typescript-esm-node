{
  "name": "@seo-practice/practice-library",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs.js",
      "types": "./lib/index.d.ts"
    }
  },
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.js",
  "source": "./src/index.ts",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "yarn build:rollup",
    "build:native": "yarn clear && concurrently \"yarn:build:native:*\"",
    "build:native:cjs": "tsc -p tsconfig.cjs.json",
    "build:native:esm": "tsc",
    "build:rollup": "yarn clear && rollup -c && tsc --emitDeclarationOnly",
    "clear": "rimraf lib"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "esbuild": "^0.15.9",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-esbuild": "^4.10.1",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=14.16"
  }
}
